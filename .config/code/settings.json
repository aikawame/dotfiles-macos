{
  "breadcrumbs.enabled": false,
  "debug.console.fontFamily": "HackGen",
  "debug.console.lineHeight": 14,
  "editor.columnSelection": false,
  "editor.detectIndentation": false,
  "editor.fontFamily": "HackGen",
  "editor.fontWeight": "bold",
  "editor.lineHeight": 14,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "alt",
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.rulers": [120],
  "editor.tabSize": 2,
  "editor.wordWrapColumn": 120,
  "files.autoSave": "afterDelay",
  "files.hotExit": "onExitAndWindowClose",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "gitlens.defaultDateFormat": "YYYY/MM/DD HH:mm:ss",
  "gitlens.defaultDateShortFormat": "YYYY/MM/DD",
  "multiCommand.commands": [
    {
      "command": "multiCommand.commentLine",
      "sequence": [
        "editor.action.commentLine",
        "cursorDown"
      ]
    },
  ],
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "ruby.intellisense": "rubyLocate",
  "ruby.rubocop.useBundler": true,
  "terminal.external.osxExec": "iTerm.app",
  "vscode_custom_css.imports": ["file:///Users/aikawame/Library/Application Support/Code/User/custom.css"],
  "vscode_custom_css.policy": true,
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Dark+ (contrast)",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.limit.value": 50,
  "workbench.editor.tabCloseButton": "off",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.statusBar.visible": true,
}
